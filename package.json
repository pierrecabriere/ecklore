    "serve": "^12.0.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:front start:back",
    "start:front": "serve -s build",
    "start:back": "node build/server.js",
    "dev": "npm-run-all --parallel dev:front dev:back",
    "dev:front": "craco start",
    "dev:back": "ts-node --compiler-options=\"{\\\"module\\\": \\\"commonjs\\\"}\" src/server.ts",
    "build:front": "craco build",
    "build:back": "tsc --module commonjs src/server.ts --outDir build",
    "build": "npm run build:front && npm run build:back",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^9.8.8",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^7.0.39",
    "prettier": "^2.4.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
